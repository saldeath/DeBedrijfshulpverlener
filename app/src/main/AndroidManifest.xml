<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="nl.debhver.debedrijfshulpverlener" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".parse.ParseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" >
        </activity>
        <activity android:name=".AdminDefaultActivity"/>
        <activity android:name=".AdminSearchActivity" />
        <activity android:name=".AdminAddUserActivity"
            android:noHistory="true">
        </activity>
        <activity android:name=".AdminAddBranchActivity"
            android:noHistory="true">
        </activity>
        <activity
            android:name=".HomeUserActivity"
            android:label="@string/title_activity_home_user"
            android:theme="@style/AppTheme" >
        </activity>
        <activity
            android:name=".AdminEquipmentAddActivity"
            android:label="@string/title_activity_admin_equipment_add"
            android:noHistory="true">
        </activity>
        <activity
            android:name=".AdminEquipmentDefaultActivity"
            android:label="@string/title_activity_admin_equipment_default" >

        </activity>
    </application>

</manifest>
